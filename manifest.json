{
   "name": "FC 3D Image Tool",
   "version": "0.9.2",
   "description": "Get fotocommunity 3D foto and convert it to different type 3D",
   "manifest_version" : 2,
   "background": {
      "scripts" : ["background.js"],
      "persistent" : true
   },
   "content_scripts": [ {
      "js": [ "contentscript.js" ],
      "matches": [ "http://www.fotocommunity.de/*" ]
   } ],
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "permissions": [ "tabs", "http://*/*" ]
}
